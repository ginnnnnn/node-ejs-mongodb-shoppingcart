<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../includes/head.ejs') %>
  </head>
  <body>
    <%- include('../includes/navigation.ejs') %>
    <main>
      <% if (orders.length <= 0) { %>
      <h1>no orders</h1>
      <% }else{ %>
      <ul>
        <% orders.forEach(order =>{ %>
        <li>
          <h1># <%= order.id %></h1>
          <ul>
            <% order.products.forEach(product =>{ %>
            <li>
              Item: <%= product.title%> Price:$<%= product.price%> Qty: <%=
              product.orderItem.quantity%>
            </li>

            <% })%>
            <p>date:<%= order.createdAt %></p>
          </ul>
        </li>
        <% }) %>
      </ul>
      <% } %>
    </main>
  </body>
</html>
